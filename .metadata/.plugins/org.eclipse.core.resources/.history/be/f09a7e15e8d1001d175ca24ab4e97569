
public class Ex04_Oper {
	public static void main(String[] args) {
		//사칙연산 + 나머지
		int value1 = 5, value2 = 2;
		//두 변수의 사칙연산결과를 출력해보기
		//연산을 먼저 하기 위한 구분자 ==> ()
		
		System.out.println("value1+value2 출력결과:"+(value1+value2));
		System.out.println("value1-value2 출력결과:"+(value1-value2));
		System.out.println("value1*value2 출력결과:"+(value1*value2));
		System.out.println("value1/value2 출력결과:"+(value1/value2));
		
		System.out.println("나머지는 % 로 구한다"+(value1%value2));
	}

}
